<%= link_to "Login", login_path %>
<%= link_to "SignUp",  new_user_path %>
<h1>Posts</h1>
<% @posts.each do |p| %>
<ul>
  <li><%= link_to  p.body, p.url %></li>
  <% p.post_votes.each do |v| %>
    Votes: <%= link_to "upvote", edit_post_post_vote_path(p.id, v.post_id) %> <%= v.score %> <%= link_to "down vote", post_post_vote_path(p.id, v.id), method: "delete" %>

  <% end %>
  <ul>
    <li><%=link_to "Comments", post_comments_path(p.id) %></li>
  </ul>
</ul>

<% end %>
